{
  "manifest_version": 3,
  "name": "Advanced Tab Manager",
  "version": "2.0",
  "description": "Tab management with grouping and dark mode",
  "icons": {
    "16": "./icons/logo.png",
    "48": "./icons/logo.png",
    "128": "./icons/logo.png"
  },
  "action": {
    "default_icon": "./icons/logo.png",
    "default_popup": "popup.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": ["tabs", "tabGroups", "storage", "sidePanel"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Open Tab Manager"
    }
  }
}
